{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/proxy",
      "methods": ["GET", "OPTIONS"],
      "dest": "server.js"
    },
    {
      "src": "/api/upload",
      "methods": ["POST", "OPTIONS"],
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}